{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "easyway.assets.manifest.schema.json",
  "type": "object",
  "additionalProperties": false,
  "required": ["version", "images"],
  "properties": {
    "version": { "const": "1" },
    "images": {
      "type": "object",
      "additionalProperties": { "type": "string", "pattern": "^/.*$" }
    }
  }
}

