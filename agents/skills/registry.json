        },
        {
          "name": "MatrixFile",
          "type": "string",
          "required": false,
          "default": "./compatibility-matrix.json",
          "description": "Path to compatibility matrix JSON file"
        },
        {
          "name": "CurrentVersions",
          "type": "hashtable",
          "required": false,
          "default": {},
          "description": "Hashtable of current versions of other components for dependency checking"
        }
      ],
      "outputs": {
        "type": "object",
        "properties": {
          "Component": {
            "type": "string"
          },
          "Version": {
            "type": "string"
          },
          "IsCompatible": {
            "type": "boolean"
          },
          "Issues": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "List of compatibility issues found"
          },
          "Recommendations": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "description": "List of recommendations for better compatibility"
          },
          "CheckedAt": {
            "type": "string",
            "format": "date-time"
          },
          "MatrixFile": {
            "type": "string"
          }
        }
      },
      "dependencies": [],
      "tags": [
        "compatibility",
        "version",
        "validation",
        "dependency"
      ],
